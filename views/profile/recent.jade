
extends ../layout

block content
    table.table.cycle.table-recent-profiles
        thead.thead
            tr
                th.th url
                th.th time
                th.th requests
                th.th size

        tbody.tbody
            each timing in timings
                tr
                    td.td
                        a(href='/profiles/#{timing.hash}') #{timing.url}
                    td.td #{timing.time}
                    td.td #{timing.requestCount}
                    td.td #{timing.weight}
